@model IEnumerable<Lunada>

@{
    ViewData["Title"]="Listado de lunadas";
}


<body>

    <div class="row">
        <div class="col-md-12">
            <a asp-action="ListaPrenada" class="btn btn-primary">
                Pre√±adas
            </a>
            <a asp-action="ListaLunada" class="btn btn-light">
                Lunadas
            </a>
        </div>
        @foreach (var i in Model)
        {
            <div class="col-md-3">
                <p>
                    @Html.DisplayFor(modelItem=>i.IdPerro)
                </p>
                <p>
                    @Html.DisplayFor(modelItem=>i.FechaInicio)
                </p>
                <p>
                    @Html.DisplayFor(modelItem=>i.FechaFin)
                </p>
                <p>
                    @Html.DisplayFor(modelItem=>i.NumeroCelos)
                </p>

            </div>
            
        }


    </div>
</body>